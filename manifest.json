{
    "manifest_version": 3,
    "name": "Custom Title Prefix",
    "description": "A Chrome extension to customize browser tab titles with a prefix.",
    "version": "1.0",
    "permissions": ["storage", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "options_ui": {
      "page": "options.html",
      "chrome_style": true
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }
  